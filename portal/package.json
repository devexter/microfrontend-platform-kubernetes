    "devDependencies": {
        "cross-var": "^1.1.0",
        "envsub": "^4.0.7"
    },
    "scripts": {
        "start": "node ./mashroom-starter",
        "docker:create-image": "source ../set-env.sh && npm install && cross-var docker build -t gcr.io/${PROJECT_ID}/mashroom-portal:latest -t gcr.io/${PROJECT_ID}/mashroom-portal:${npm_package_version} .",
        "docker:push": "source ../set-env.sh && npm run docker:create-image && docker push gcr.io/${PROJECT_ID}/mashroom-portal:latest && cross-var docker push gcr.io/$PROJECT_ID/mashroom-portal:${npm_package_version}",
        "docker:start": "source ../set-env.sh && docker run -e NODE_ENV=production -p 5050:5050 gcr.io/${PROJECT_ID}/mashroom-portal",
        "deploy": "npm run docker:push && cd kubernetes && ./deploy.sh"
    }
}
